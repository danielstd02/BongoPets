{
    "name": "bongopets/laravel",
    "description": "Un proyectopara gestionar servicios de peluqueria para mascotas.",
    "type": "project",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.1",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.8"
    },
    "require-dev": {
        "facade/ignition": "^2.16",
        "laravel/sail": "^1.16",
        "laravel/testbench": "^6.0",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
